<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bitdevices.Resume" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Marcus Meneses</name>
    <description>
        My resum√© in application format.
    </description>
    <author email="marcusmeneses.email@gmail.com" href="http://8bitdevices.com.br">
        Marcus Meneses
    </author>
    <icon src="res/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="res/logo.png" />
    <splash density="land-ldpi" src="res/logo.png" />
    <splash density="land-mdpi" src="res/logo.png" />
    <splash density="land-xhdpi" src="res/logo.png" />
    <splash density="land-xxhdpi" src="res/logo.png" />
    <splash density="port-hdpi" src="res/logo.png" />
    <splash density="port-ldpi" src="res/logo.png" />
    <splash density="port-mdpi" src="res/logo.png" />
    <splash density="port-xhdpi" src="res/logo.png" />
    <splash density="port-xxhdpi" src="res/logo.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="true" />
    <preference name="SplashScreenDelay" value="100" />
    <preference name="AutoHideSplashScreen" value="false" />
</widget>
